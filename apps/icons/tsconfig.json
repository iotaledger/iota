{
    "compilerOptions": {
        "noEmit": true,
        "target": "ESNext",
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src"]
}
