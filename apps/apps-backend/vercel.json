{
    "version": 2,
    "builds": [
        {
            "src": "src/vercel-main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/vercel-main.ts",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        }
    ]
}
